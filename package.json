{
  "name": "symbol-namer",
  "description": "A plugin to help you rename symbol instances. Rename symbol instances to their override text values, or set and use a default name.",
  "author": "Giles Perry <giles@gilesperry.info> (http://gilesperry.info/)",
  "version": "2.0.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Symbol Namer",
    "manifest": "src/manifest.json",
    "identifier": "com.gilesperry.symbol-namer",
    "main": "symbol-namer.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perrysmotors/symbol-namer.git"
  },
  "license": "MIT"
}
